
{% block channelDescriptions %}
    {% dump %}
<table class="table channel-descriptions">
    <tbody>
        <tr class="">
            <th>
                {{ 'librinfo.label.channel'|trans() }}
            </th>
            <th>
                {{ 'Value'|trans() }}
            </th>
        </tr>
    {% for description in value %}
        <tr class="sonata-ba-view-container">
            <th>
                {{ description.channel.name }}
            </th>
            <td>
                {{ description.value }}
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{% endblock %}