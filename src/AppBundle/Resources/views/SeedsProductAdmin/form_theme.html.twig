{% extends 'BlastCoreBundle:Admin/Form:fields.html.twig' %}

{% block data %}
    {% if sonata_admin.admin.classnameLabel == 'SeedsProductVariant' %}
        data = [
           {
            name:  '{{sonata_admin.admin.uniqid}}_seedBatch',
            value: $('input[name="{{ form.parent.vars.full_name }}[seedBatch]"]').val()
           },{
            name:  '{{sonata_admin.admin.uniqid}}_packaging',
            value: $('select[name="{{ form.parent.vars.full_name }}[packaging]"]').val()
           }
        ];
    {% else %}
        {{ parent() }}
    {% endif %}
{% endblock %}
