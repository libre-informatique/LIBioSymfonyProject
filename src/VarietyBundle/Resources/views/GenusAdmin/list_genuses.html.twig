{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {% for genus in value %}
        {% spaceless %}
        <a href="{{ path('admin_variety_genus_show', {id:genus.id}) }}">
            {{ genus }}
        </a>{% if not loop.last %},&nbsp;{% endif %}
        {% endspaceless %}
    {% endfor %}
{% endblock %}
