{
    "name": "libre-informatique/libio-project",
    "license": "GPL-3.0",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"          
        },
        "classmap": ["app/VendorOverride/", "app/AppKernel.php", "app/AppCache.php"]        
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^5.6|^7.0",
        "symfony/symfony": "2.8.x-dev",
        "symfony/monolog-bundle": "^2.8",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/assetic-bundle": "^2.8",
        "sensio/framework-extra-bundle": "^3.0",
        "sensio/distribution-bundle": "~3.0",
        "sensio/generator-bundle": "^3.0",
        "doctrine/orm": "^2.4.8,<2.5",
        "doctrine/doctrine-bundle": "dev-master",
        "sonata-project/core-bundle": "^3.0",
        "sonata-project/admin-bundle": "^3.4",
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "sonata-project/easy-extends-bundle": "^2.1",
        "sonata-project/intl-bundle": "^2.2",
        "sonata-project/datagrid-bundle": "^2.2",
        "sonata-project/user-bundle": "^3.0",
        "nelmio/api-doc-bundle": "^2.13",
        "friendsofsymfony/rest-bundle": "^1.7",
        "incenteev/composer-parameter-handler": "^2.1",
        "jms/security-extra-bundle": "^1.6",
        "libre-informatique/email-crm-bundle": "dev-master",
        "libre-informatique/ui-bundle": "dev-master",
        "libre-informatique/varieties-bundle": "dev-master",
        "libre-informatique/seed-batch-bundle": "dev-master",
        "libre-informatique/security-bundle": "dev-master",
        "libre-informatique/user-bundle": "dev-master",
        "libre-informatique/doctrine-pgsql-bundle": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "jms/serializer-bundle": "^1.1",
        "libre-informatique/media-bundle": "dev-master",
        "libre-informatique/product-bundle": "dev-master",
        "sylius/sylius": "^1.0"
    },
    "require-dev": {
        "alexandresalome/doctrine-extra-bundle": "^0.3.0",
        "hautelook/alice-bundle": "^1.3.1"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Librinfo\\CoreBundle\\Tools\\Patches\\Patcher::applyPatches"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Librinfo\\CoreBundle\\Tools\\Patches\\Patcher::applyPatches"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
