{
    "name": "libre-informatique/seed-batch-bundle",
    "type": "symfony-bundle",
    "description": "Seed batch Bundle for Symfony",
    "require": {
        "php": ">=5.6",
        "symfony/symfony": ">=3.2",
        "blast-project/core-bundle": ">=0.4",
        "blast-project/utils-bundle": ">=0.4",
        "blast-project/base-entities-bundle": ">=0.4",
        "libre-informatique/media-bundle": ">=0.4",
        "libre-informatique/varieties-bundle": ">=0.4"
    },
    "require-dev": {
	"symfony/phpunit-bridge": ">=3.2"
    },
    "license": "LGPL-3.0",
    "keywords": [
        "e-venement",
        "libio",
        "sonata",
        "symfony"
    ],
    "homepage": "https://github.com/libre-informatique/SymfonyLibrinfoSeedBatchBundle",
    "authors": [
        {
            "name": "Marcos BEZERRA DE MENEZES",
            "email": "marcos.bezerra@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev",
            "dev-wip-lisem": "0.4.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Librinfo\\SeedBatchBundle\\": "src/",
            "Librinfo\\SeedBatchBundle\\Tests\\": "tests/"
        }
    }
}
